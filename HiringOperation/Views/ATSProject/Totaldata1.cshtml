                <th>
                <span style="font-size: 5px">HallTicket</span>
                </th>
                <th>
                <span style="font-size: 5px">EmailID</span>
                </th>
                <th>
                <span style="font-size: 5px">Dob</span>
                </th>
                <th>
                <span style="font-size: 5px">Gender</span>
                </th>
                <th>
                <span style="font-size: 5px">StudentPHno</span>
                </th>
                <th>
                <span style="font-size: 5px">StudentAadhar</span>
                </th>
                <th>
                <span style="font-size: 5px">SchoolName</span>
                </th>
                <th>
                <span style="font-size: 5px">SscPassYear</span>
                </th>
                <th>
                <span style="font-size: 5px">SscAggregate</span>
                </th>
                <th>
                <span style="font-size: 5px">CollegeName</span>
                </th>
                <th>
                <span style="font-size: 5px">IntermediatePassYear</span>
                </th>
                <th>
                <span style="font-size: 5px">IntermediateAggregate</span>
                </th>
                <th>
                <span style="font-size: 5px">EngineeringCollegeName</span>
                </th>
                <th>
                <span style="font-size: 5px">Branch</span>
                </th>
                <th>
                <span style="font-size: 5px">EngineeringPassout</span>
                </th>

                <th>
                <span style="font-size: 5px">Backlog</span>
                </th>

                <th>
                <span style="font-size: 5px">GraduationAggregate</span>
                </th>

                <th>
                <span style="font-size: 5px">FathersName</span>
                </th>

                <th>
                <span style="font-size: 5px">PermanentAddress</span>
                </th>

                <th>
                <span style="font-size: 5px">FathersMobileNo</span>
                </th>

                <th>
                <span style="font-size: 5px">MothersName</span>
                </th>

                <th>
                <span style="font-size: 5px">FathersMobileNo</span>
                </th>

                <th>
                <span style="font-size: 5px">MothersOccupation</span>
                </th>




                <th style="width: 5%">
                <span style="font-size: 15px">Action</span>
                </th>
                </tr>
                </thead>

                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Sno</td>
                                <td>@item.HallTicket</td>
                                <td>@item.Name</td>
                                <td>@item.EmailID</td>
                                <td>@item.Dob</td>
                                <td>@item.Gender</td>
                                <td>@item.StudentPHno</td>
                                <td>@item.StudentAadhar</td>
                                <td>@item.SchoolName</td>
                                <td>@item.SscPassYear</td>
                                <td>@item.SscAggregate</td>
                                <td>@item.CollegeName</td>
                                <td>@item.IntermediatePassYear</td>
                                <td>@item.IntermediateAggregate</td>
                                <td>@item.EngineeringCollegeName</td>
                                <td>@item.Branch</td>
                                <td>@item.EngineeringPassout</td>
                                <td>@item.Backlog</td>
                                <td>@item.GraduationAggregate</td>

                                <td>@item.FathersName</td>
                                <td>@item.PermanentAddress</td>
                                <td>@item.FathersMobileNo</td>
                                <td>@item.MothersName</td>
                                <td>@item.MothersOccupation</td>


                            </tr>
                        }

                    </table>

